<div class="signup">
	<h2>Sign up</h2>

	<% resource.build_schedule %>

	<%= form_for(resource, :controllers => {:registrations => "newregistrations" }, :as => resource_name, :url => registration_path(resource_name) ,  :validate => true) do |f| %>
	 
	   <%#= f.error_notification %>
	   <%= render :partial => 'shared/error_messages', :locals => {:target => resource}%>

	   <%= render :partial => 'form', :locals => {:f => f}%>

	   <div class="form-actions">
	    <%= f.submit "Sign up" %>
	   </div>
	<% end %>

	<%= render "devise/shared/links" %>

</div>


